/*1~9字体*/
@font-face {
    font-family: 'DINPro';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: bold;
    font-style: normal;
}

.p-title{
  font-size: 36rpx;
  color: #031331;
  text-align: center;
  font-weight: 600;
  margin-bottom: 40rpx;
  margin-top: 60rpx;
  line-height: 1;
}

.list{
  
}

.list .item{
  background: #fff;
  border-radius: 10rpx;
  padding:34rpx 30rpx;
  margin-bottom: 20rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  /* padding:20rpx; */
  box-shadow: 0 0 15rpx 0 rgba(152,152,152,0.15);
}

.list .item .watches{
  width: 53rpx;
  height: 93rpx;
  margin: 0;
  margin-right: 30rpx;
  display: block;
}
.list .item .title{
  text-align: left;
  line-height: 1;
  font-size: 28rpx;
  color: #282828;
  letter-spacing: 1px;
  font-weight: 600;
  margin-top: 15rpx;
}
.list .item .bind-code{
  line-height: 1;
  font-size: 24rpx;
  color: #000000;
  letter-spacing: 1px;
  margin-top: 20rpx;
}
.list .item .watches-name{
  line-height: 1;
  text-align: right;
  font-size: 24rpx;
  color: #ADADAD;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}

.list .item .select{
  background: #EDEDED;
  border: 1px solid #ADADAD;
  border-radius: 10em;
  text-align: center;
  width: 40rpx;
  height: 40rpx;
  float: right;
}
.list .item  .select .iconfont{
  color: #EDEDED;
  line-height: 38rpx;
  font-size: 20rpx;
}
.list .item  .select.active{
  background: #031331;
  border: 1px solid #031331;
}
.list .item  .select.active .iconfont{
  color: #fff;

}
.bottom-btn-group{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 360rpx;
  background: #fff;
  box-shadow: 0 0 15rpx 0 rgba(152,152,152,0.15);
}
.bottom-btn-group .btn-select{
  background: #031331;
  border: 1px solid #031331;
  border-radius: 50rpx;
  width: calc(100% - 40rpx);
  line-height: 100rpx;
  margin:40rpx 20rpx 20rpx 20rpx;
  text-align: center;
  color: #fff;
  font-size: 40rpx;
}
.bottom-btn-group .btn-scan{
  background: #fff;
  border: 1px solid #031331;
  border-radius: 50rpx;
  width: calc(100% - 40rpx);
  line-height: 100rpx;
  margin:0 20rpx 40rpx 20rpx;
  text-align: center;
  color: #031331;
  font-size: 40rpx;
}

.dot-group{
  margin:0 auto;
  width: 88rpx;
}
.dot-group .dot{
  background: #EDEDED;
  border-radius: 10em;
  width: 15rpx;
  height: 15rpx;
  float: left;
  margin:0 15rpx;

}
.dot-group .dot.active{
  background: #031331;
}
.add-user .title{
  margin-top: 80rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #031331;
  text-align: center;
}
.add-user .input{
  width: 100%;
  margin-top: 60rpx;
  margin-bottom: 20rpx;
}
.add-user .sex .item{
  width: 50%;
  float: left;
  text-align: center;
  margin-top: 40rpx;
}
.add-user .sex .item .iconfont{
  font-size: 90rpx;
  color: #ADADAD;
}
.add-user .sex .item .text{
  color: #ADADAD;
}
.add-user .sex .item.active .text{
  color: #031331;
}
.add-user .sex .item.active:nth-child(1) .iconfont{
  color: #3D88E0;
}
.add-user .sex .item.active:nth-child(2) .iconfont{
  color: #E8696A;
}
.add-user .input .name{
  width: 50%;
  border-bottom: 1px solid #ccc;
  color: #ADADAD;
  margin:0 auto;
  font-size: 42rpx;
  text-align: center;
  float: none;
  height: 60rpx;
  line-height: 60rpx;
}
.add-user .bind-code{
  margin-top: 30rpx;
  font-family: 'DINPro';
  font-size: 48rpx;
  color: #282828;
  letter-spacing: 1.6rpx;
  text-align: center;
}
.add-user .btn-add{
  font-size: 40rpx;
  color: #fff;
  background: #031331;
  border-radius: 50rpx;
  width: 80%;
  margin-left: 10%;
  margin-top: 100rpx;
  line-height: 100rpx;
  margin-bottom: 70rpx;


}